{
    "env": {
        "global.dataset.alphavantage.apikey": "ALPHAVANTAGE_APIKEY"
    },

    "global": {
        "dataset": {
            "alphavantage": {
                "datatype": "csv"
            },

            "scale_real_columns": [ ],
            "scale_categorical_columns": [ ],
            "embed_integer_columns": [ ]
        },
        "model": {
            "test_split": 0.1,
            "validation_split": 0.2,
            "batch_size": 64,
            "epochs": 4,
            
            "hidden_layer_size": 128,
            "dropout_rate": 0.2,
                
            "deepspeed": {
                "gradient_accumulation_steps": 1,
                "optimizer": {
                    "type": "Adam",
                    "params": {
                      "lr": 0.001,
                      "betas": [
                        0.8,
                        0.999
                      ],
                      "eps": 1e-8,
                      "weight_decay": 3e-7
                    }
                },
                "scheduler": {
                    "type": "WarmupLR",
                    "params": {
                        "warmup_min_lr": 0.00001,
                        "warmup_max_lr": 0.001,
                        "warmup_num_steps": 25000
                    }
                },
                "comms_logger": {
                    "enabled": false,
                    "verbose": false,
                    "prof_all": false,
                    "debug": false
                },
                "zero_optimization": true,
                "steps_per_print": 250
            }
        }
    },

    "runs": {
        "Intra-LSTM-TQQQ": {
            "neptune": {
                "name": "Intra-LSTM",
                "with_id": "STOC-1",
                "tags": [ "Intraday", "LSTM", "Stock" ]
            },

            "dataset_name": "alphavantage",
            "dataset": {
                "alphavantage": {
                    "symbol": "TQQQ",

                    "function": "TIME_SERIES_INTRADAY",
                    "months": "-8",
                    "interval": "5min",
                    "outputsize": "full",
                    "extended_hours": false
                },
                "indicators": [
                    {
                        "function": "EMA",
                        "period": 20,
                        "is_input": true,
                        "is_scaled": true
                    },
                    {
                        "function": "MACD",
                        "period": 26,
                        "period2": 12,
                        "is_input": true,
                        "is_scaled": true
                    },
                    {
                        "function": "delta_close",
                        "is_input": true
                    },
                    {
                        "function": "log_vol",
                        "is_input": true
                    }
                ],
                "columns": [
                    { "name": "close", "is_scaled": true },
                    { "name": "dt_hour", "is_scaled": true }
                ]
            },

            "model_name": "SimpleLSTM",
            "model": {
                "epochs": 8,
                "seq_len": 72,
                "out_seq_len": 1,
            
                "hidden_layer_size": 256
            },

            "trader_name": "StandardTrader",
            "trader": {
                "episodes": 256,
                "hidden_layer_size": 2048
            }
        },
        "Intra-Ext-LSTM-TQQQ": {
            "copy_run": "Intra-LSTM-TQQQ",
            "dataset": {
                "alphavantage": {
                    "months": "-24"
                }
            }
        },
        "Intra-GMLP-TQQQ": {
            "copy_run": "Intra-LSTM-TQQQ",
            "model_name": "GatedMLP",
            "model": {
                "epochs": 1,
                "hidden_layer_size": 128,
                "dropout_rate": 0.0,
                "learning_rate": 0.001,

                "seq_len": 72,
                "out_seq_len": 1,

                "precompile": false,
                "pin_memory": true,

                "gmlp": {
                    "embedding_length": 32,
                    "layer_count": 8
                }
            },
            "trader": {
                "episodes": 0,
                "hidden_layer_size": 2048
            }
        },
        "Intra-GMLP128-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model": {
                "epochs": 16,
                "hidden_layer_size": 128
            }
        },
        "Intra-GMLP256-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model": {
                "epochs": 24,
                "hidden_layer_size": 256
            }
        },
        "Intra-GMLP-MADL-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model": {
                "loss": "mse + madl"
            }
        },
        "Intra-GMLP-Testing-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model": {
                "epochs": 1
            }
        },
        "Intra-GCNN-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model_name": "GatedCNN",
            "model": {
                "epochs": 8
            },
            "gmlp": {
                "embedding_length": 48,
                "layer_count": 4
            }
        },
        "Intra-GCNN-Testing-TQQQ": {
            "copy_run": "Intra-GMLP-TQQQ",
            "model_name": "GatedCNN",
            "model": {
                "epochs": 1
            }
        },
        "Intra-Ext-GMLP-TQQQ": {
            "copy_run": "Intra-Ext-LSTM-TQQQ",
            "model_name": "GatedMLP",
            "model": {
                "epochs": 12,
                "hidden_layer_size": 128,
                "gmlp": {
                    "embedding_length": 128,
                    "layer_count": 8
                }
            }
        },



        "Daily-LSTM-TQQQ": {
            "neptune": {
                "name": "Daily-LSTM",
                "with_id": "STOC-1",
                "tags": [ "Intraday", "LSTM", "Stock" ]
            },

            "dataset_name": "alphavantage",
            "dataset": {
                "alphavantage": {
                    "symbol": "TQQQ",

                    "function": "TIME_SERIES_DAILY_ADJUSTED",
                    "outputsize": "full"
                },
                "indicators": [
                    {
                        "function": "EMA",
                        "period": 20,
                        "is_input": true,
                        "is_scaled": true
                    },
                    {
                        "function": "MACD",
                        "period": 26,
                        "period2": 12,
                        "is_input": true,
                        "is_scaled": true
                    },
                    {
                        "function": "delta_close",
                        "is_input": true
                    },
                    {
                        "function": "log_vol",
                        "is_input": true
                    }
                ],
                "columns": [
                    { "name": "close", "is_scaled": true },
                    { "name": "dt_hour", "is_scaled": true }
                ]
            },

            "model_name": "SimpleLSTM",
            "model": {
                "epochs": 8,
                "seq_len": 72,
                "out_seq_len": 1,
            
                "hidden_layer_size": 256
            },

            "trader_name": "StandardTrader",
            "trader": {
                "episodes": 256,
                "hidden_layer_size": 2048
            }
        }
    }
}